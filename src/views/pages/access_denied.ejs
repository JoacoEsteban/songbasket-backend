html{
  font-size: 30px;
}
body{
    background: black;
    color: #ccc;
    /* font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif; */
    font-family: 'Poppins', sans-serif;
    font-weight: 400;
    font-size: 1rem;
    text-align: center;
    margin: 0;
    user-select: none;
  }
  .button{
    display: inline-block;
    outline: none;
    font-family: 'Poppins', sans-serif;
    font-weight: 500;
    text-decoration: none;
    color: black;
    font-size: 0.7rem;
    padding: .5rem 1rem;
    /* height: 2rem; */
    /* width: 10rem; */
    border: none;
    border-radius: 2rem;
    background: #fff;
    transition: transform .15s ease, background .2s ease;
}
.button:hover{
    transform: scale(1.05);
    background: #9900cc;
    color: #fff;
}
.dev-border-w{
    border: 1px solid white;
}
.dev-border-b{
    border: 1px solid black;
}
    
